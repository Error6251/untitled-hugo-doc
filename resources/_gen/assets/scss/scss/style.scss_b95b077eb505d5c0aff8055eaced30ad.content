@import "https://fonts.googleapis.com/css?family=Poppins:400,500,600,700,800,900|Yeseva+One&display=swap";@import "https://fonts.googleapis.com/css2?family=Jost:wght@300;400;500;600;700&family=Poppins&display=swap";@import "https://fonts.googleapis.com/css?family=Poppins:400,500,600,700,800,900|Yeseva+One&display=swap";@import "https://fonts.googleapis.com/css?family=Poppins:400,500,600,700,800,900|Yeseva+One&display=swap";body{font-family:poppins,sans-serif;font-size:14px;color:#282f49;background:#fff}h1,h2,h3,h4,h5,h6{color:#282f49;font-family:poppins,sans-serif;font-weight:700;letter-spacing:.5px}a{font-family:poppins,sans-serif}p{font-size:16px;color:#282f49;line-height:26px}h1{font-size:50px;line-height:70px}h2{font-size:40px;line-height:55px}h3{font-size:20px}h4{font-size:20px}.btn{display:inline-block;padding:15px 30px;font-size:16px;border-radius:7px}.btn-lg{padding:20px 50px}.btn-light{color:#5d78ff}.btn-light:hover{color:#5d78ff}.btn-zoom{transition:all .3s ease;transform:scale(1)}.btn-zoom:hover{box-shadow:0 8px 20px rgba(56,87,241,.3);transform:scale(1.03)}body{padding:0;margin:0}.main-wrapper{display:flex}.main-wrapper aside{padding:20px;border-right:solid 1px #282f49;min-height:calc(100vh - 200px)}.main-wrapper main{padding:0 20px 20px;flex:1}.container{padding:0 50px}.navbar{padding:30px 20px;background:#cbcbcb;transition:all .3s ease;border-bottom:solid 1px #000;display:flex;align-items:center}.navbar .logo{flex:1;font-size:xx-large}.navbar .logo a{font-weight:900;color:#282f49;text-decoration:none}.navbar .search{width:300px;height:25px;margin-right:50px;position:relative}.navbar .search-input{width:calc(100% - 22px);padding:10px}.navbar .search-items{position:absolute;list-style:none;padding:0;top:25px;width:100%;height:280px;overflow:auto}.navbar .search-items li{padding:5px;background-color:#fff;border:solid 1px #000}.navbar .search-items li a{display:block;text-decoration:none;color:#282f49}.navbar .search-items li:hover{background-color:#f0f8ff}.footer{padding:10px 0;background:#cbcbcb;text-align:center;transition:all .3s ease}.sidebar ul{padding:0}.sidebar ul li{list-style:none}.sidebar a{display:block;text-decoration:none;padding:0 10px;color:#5d78ff;margin:5px 0}.sidebar a.active{color:#282f49;font-weight:700}.sidebar a.active::before{content:"|"}.sidebar .sidebar-sub-menu{margin-right:20px}.sidebar .sidebar-sub-menu .mark{padding:0 6px;margin-left:5px}.sidebar .sidebar-sub-menu-child{padding-left:20px}.sidebar .sidebar-sub-menu.parent .sidebar-sub-menu-child{display:block}.sidebar .sidebar-sub-menu.nope .mark{padding:0 5px}.sidebar .sidebar-sub-menu.nope .sidebar-sub-menu-child{display:none}.prism{position:relative}.prism-btn{position:absolute;top:20px;right:40px}.prism-code{overflow:scroll;max-height:300px}