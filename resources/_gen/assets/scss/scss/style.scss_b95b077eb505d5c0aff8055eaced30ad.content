@import "https://fonts.googleapis.com/css?family=Poppins:400,500,600,700,800,900|Yeseva+One&display=swap";@import "https://fonts.googleapis.com/css2?family=Jost:wght@300;400;500;600;700&family=Poppins&display=swap";body{font-family:poppins,sans-serif;font-size:14px;color:#282f49;background:#fff}h1,h2,h3,h4,h5,h6{color:#282f49;font-family:poppins,sans-serif;font-weight:700;letter-spacing:.5px}a{font-family:poppins,sans-serif}p{font-size:16px;color:#282f49;line-height:26px}h1{font-size:50px;line-height:70px}h2{font-size:40px;line-height:55px}h3{font-size:20px}h4{font-size:20px}.btn{display:inline-block;padding:15px 30px;font-size:16px;border-radius:7px}.btn-lg{padding:20px 50px}.btn-light{color:#5d78ff}.btn-light:hover{color:#5d78ff}.btn-zoom{transition:all .3s ease;transform:scale(1)}.btn-zoom:hover{box-shadow:0 8px 20px rgba(56,87,241,.3);transform:scale(1.03)}body{padding:0;margin:0}.container{padding:0 50px}.navbar{padding:30px 20px;background:#cbcbcb;transition:all .3s ease;border-bottom:solid 1px #000}.navbar .logo{font-size:xx-large}.footer{padding:10px 0;background:#cbcbcb;text-align:center;transition:all .3s ease}.sidebar ul li{list-style:none}